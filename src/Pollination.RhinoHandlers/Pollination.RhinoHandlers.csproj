		<PackageReference Include="RhinoCommon" Version="6.23.20055.13111" IncludeAssets="compile;build" />
	</ItemGroup>

	<ItemGroup Condition="'$(Configuration)'!='Debug_packages'">
		<PackageReference Include="Pollination.Core" Version="1.6.1" IncludeAssets="compile;build" />
		<PackageReference Include="PollinationSDK" Version="0.23.0.3" IncludeAssets="compile;build" />
	</ItemGroup>

	<ItemGroup Condition="'$(Configuration)'=='Debug_packages'">
		<ProjectReference Include="..\..\..\rhino-plugin-core\src\Pollination.Core\Pollination.Core.csproj" />
		<ProjectReference Include="..\..\..\pollinationSDK\src\PollinationSDK\PollinationSDK.csproj" />
	</ItemGroup>

</Project>
